{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Frontend",
  "dependencies": {
    "@graphql-codegen/typescript-resolvers": "^1.8.3",
    "aws-amplify": "^3.0.0",
    "aws-amplify-react": "^4.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^6.0.0",
    "react-scripts": "^3.2.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1"
  },
  "devDependencies": {
    "@anttiviljami/serverless-stack-output": "^0.3.1",
    "@graphql-codegen/cli": "^1.8.3",
    "@graphql-codegen/typescript": "^1.8.3",
    "@graphql-codegen/typescript-operations": "^1.8.3",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^10.0.0",
    "@types/jest": "^25.0.0",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^6.0.0",
    "@types/react-test-renderer": "^16.9.1",
    "@types/zen-observable": "^0.8.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "js-yaml": "^3.13.1",
    "minimist": "^1.2.0",
    "monitoring-appsync-service": "file:./../services/monitoring-appsync-service",
    "monitoring-tester-service": "file:./../services/monitoring-tester-service",
    "react-test-renderer": "^16.11.0",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-s3-remover": "^0.6.0",
    "serverless-scriptable-plugin": "1.0.5",
    "serverless-single-page-app-plugin": "file:./serverless-single-page-app-plugin",
    "source-map-explorer": "^2.1.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "analyze": "source-map-explorer",
    "start": "react-scripts start",
    "build": "react-scripts build && node postBuild.js",
    "test": "TZ=GMT CI=true react-scripts test --verbose --env=jest-environment-jsdom-sixteen",
    "test:watch": "TZ=GMT react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:ci": "TZ=GMT CI=true JEST_JUNIT_OUTPUT_DIR=../reports/junit JEST_JUNIT_OUTPUT_NAME=${npm_package_name}-test-results.xml yarn test --runInBand --ci --reporters=jest-junit --reporters=default",
    "eject": "react-scripts eject",
    "lint": "tslint 'src/**/*.ts?(x)'",
    "coverage": "TZ=GMT yarn test --coverage",
    "deploy": "node deploy.js",
    "deploy:stack": "serverless deploy",
    "remove": "serverless remove",
    "publish": "serverless publishSite",
    "prettier": "prettier --write 'src/**/*.ts' 'src/**/*.tsx' 'serverless-single-page-app-plugin/*.js' 'scripts/*.js'",
    "prettier:ci": "prettier --list-different 'src/**/*.ts' 'src/**/*.tsx' 'serverless-single-page-app-plugin/*.js' 'scripts/*.js'",
    "print:name": "echo ${npm_package_name}",
    "graphql:codegen": "graphql-codegen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "author": "Erez Rokah",
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!/node_modules/",
      "!src/index.tsx",
      "!src/components/index.tsx",
      "!src/serviceWorker.ts",
      "!src/setupTests.ts"
    ]
  }
}
